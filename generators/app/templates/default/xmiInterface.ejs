import {Promise} from 'es6-promise';

export interface <%= name %>Contract {
  <% (attributes || []).forEach(function (attr) { %>
    /**
    * <%= attr.name %> property
    */
    <%= attr.name %>: <%= attr.type %>;
  <% }) %>

  <% (operations || []).forEach(function (operation) { %>
    /**
    * <%= operation.name %> description.
    */
    <%= operation.name %>(state: any): Promise<any>;
  <% }) %>
}
