<% var normlalize = function(str) { return str && str
        .replace(/\n/g, ". ")
        .replace(/\\/g, "\\\\")
        .replace(/\"/g, '\\"')
        .replace(/\r/g, "\\r")
        .replace(/\t/g, "\\t")
        .replace(/\f/g, "\\f");
} %>

{
    "name": "<%- normlalize(entity.nameOrigin) %>",
    "description": "<%- normlalize(entity.description) %>",
    "basicPathScenarios": [<%- (entity.basicPathScenarios || []).map(x => `"${normlalize(x)}"`) %>]
}