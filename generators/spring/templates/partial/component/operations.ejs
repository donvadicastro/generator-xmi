<% (entity.operations || []).forEach(function (operation) { %>
    /**
    * <%= operation.nameOrigin %> action.<% if(operation.description) { %>
    * <%- operation.description.replace(/\n/g, '\n* ') %>.<% } %>
    */
    public <%- include('../interface/operation', {operation: operation}) -%> {
        <% if(operation.returnParameter.type !== 'void') { %>
            return <%- operation.isReturnArray ? 'Collections.emptyList()' : operation.returnParameter.typeDefaultValue %>;
        <% } %>
    }
<% }) %>
