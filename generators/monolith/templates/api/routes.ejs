import { Application } from 'express';

<% value.forEach(x => { %>
import <%= x.entity.name %>Router from './routes<%= x.path %>/router';
<% }) %>

export default function routes(app: Application): void {
<% value.forEach(x => { %>
    app.use('/api/v1<%= x.url %>', <%= x.entity.name %>Router);
<% }) %>
};