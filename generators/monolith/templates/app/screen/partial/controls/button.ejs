<div class="form-group">
    <% if(entity.links.informationFLow.length && entity.links.informationFLow[0].end) {%>
        <% var elementRefPath = entity.links.informationFLow[0].end.elementRef.pathFromRoot %>
        <button class="btn btn-primary" (click)="run('<%= elementRefPath.length ? utils.kebabCase(elementRefPath.slice(0, -1)) : elementRefPath %>')"><%= entity.properties.get('label') || entity.name %></button>
    <% } else { %>
        <button class="btn btn-primary" disabled>Run</button>
    <% } %>
</div>
