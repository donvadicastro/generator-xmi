<label><%= entity.properties.get('label') || entity.name %></label>
<table class="table">
    <thead class="thead-dark">
        <tr>
        <% (entity.children || []).forEach(x => { %>
            <th><%= x.properties.get('label') || x.name %></th>
        <% }) %>
        </tr>
    </thead>
    <tbody formArrayName="<%= entity.name %>">
        <tr *ngFor="let item of form.get('<%= entity.name %>').controls; let i = index;" [formGroupName]="i">
        <% (entity.children || []).forEach(x => { %>
        <% if(x.stereotype === 'label') { %>
            <td>{{ form.get('<%= entity.name %>').value[i].<%= x.name %> }}</td>
        <% } else if(x.stereotype === 'button') { %>
                <td><button class="btn btn-primary"><%= x.properties.get('label') || x.name %></button></td>
        <%} else { %>
                <td><input formControlName="<%= x.name %>"/></td>
        <% } %>
        <% }) %>
        </tr>
    </tbody>
</table>
