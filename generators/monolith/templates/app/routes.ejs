import { NgModule } from '@angular/core';
import { Routes, RouterModule } from '@angular/router';

<% classes.forEach(x => { %>
import {<%= x.entity.name %>ListComponent} from './administration/<%= x.path %>/<%= x.entity.name %>/list/component';
import {<%= x.entity.name %>EditComponent} from './administration/<%= x.path %>/<%= x.entity.name %>/edit/component';
<% }) %>

const routes: Routes = [
  { path: 'administration',  children: [
    <% classes.forEach(x => { %>
        { path: '<%= x.entity.pathFromRoot %><%= x.entity.name %>',  component: <%= x.entity.name %>ListComponent },
        { path: '<%= x.entity.pathFromRoot %><%= x.entity.name %>/:id',  component: <%= x.entity.name %>EditComponent },
        { path: '<%= x.entity.pathFromRoot %><%= x.entity.name %>/new',  component: <%= x.entity.name %>EditComponent },
    <% }) %>
    ] }
];

@NgModule({
  imports: [RouterModule.forRoot(routes)],
  exports: [RouterModule]
})
export class AppRoutingModule { }
