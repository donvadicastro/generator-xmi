{
   "type": "mongodb",
   "host": "<host>",
   "port": 1234,
   "username": "<username>",
   "password": "<password>",
   "database": "<database>",
   "synchronize": true,
   "logging": false,
   "entities": [<% classes.forEach((x, i, list) => { %>
      "design<%= x.path %>/components/generated/<%= x.entity.name %>.generated.ts"<% if(list.length-1 > i) {%>,<%}%><% }) %>
   ],
   "subscribers": [<% classes.forEach((x, i, list) => { %>
      "design<%= x.path %>/components/generated/<%= x.entity.name %>.generated.ts"<% if(list.length-1 > i) {%>,<%}%><% }) %>
   ],
   "cli": {
      "entitiesDir": "src/entity",
      "migrationsDir": "src/migration",
      "subscribersDir": "src/subscriber"
   }
}
