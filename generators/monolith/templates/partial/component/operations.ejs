<% (entity.operations || []).forEach(function (operation) { %>
    /**
    * <%= operation.nameOrigin %> action.<% if(operation.description) { %>
    * <%- operation.description.replace(/\n/g, '\n* ') %>.<% } %>
    */
    async <%= operation.name %>(<% operation.inputParameters.forEach(x => { %>
                <%= x.name %>: <%= x.typeRef ? `${x.typeRef.namePascal}${x.isArray ? '[]' : ''}` : x.type %>,
            <% }) %>): <%= operation.returnParameter.typeRef ? `${operation.returnParameter.typeRef.namePascal}` : operation.returnParameter.type %><%= operation.isReturnArray ? '[]' : '' %> | null {

        return await null;
    }
<% }) %>
