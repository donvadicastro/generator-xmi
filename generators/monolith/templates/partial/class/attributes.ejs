<% if (orm && !entity.generalizationLinksTo) { %>
    @PrimaryGeneratedColumn()
    id?: number;
<% } %>

<% (entity.attributes || []).forEach(function (attr) { %>
    <% if (orm) { %>
        <% if(attr.isEnum) { %>
            @Column('int')
        <% } else if(attr.isDataType) { %>
            @Column('json')
        <% } else if(attr.typeRef) { %>
            @ManyToOne(type => <%= attr.typeRef.namePascal %>, {nullable: false})
            @JoinColumn()
        <% } else { %>
            @Column('<%= attr.dbType %>')
        <% }%>
    <% } %>
    <%= attr.name %>: <%= attr.typeRef ? attr.typeRef.namePascal : attr.type %>;
<% }) %>
