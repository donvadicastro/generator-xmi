<% (entity.operations || []).forEach(function (operation) { %>
    /**
    * <%= operation.name %> description.
    */
    async <%= operation.name %>(state: FlowStateType & {
            <% operation.inputParameters.forEach(x => { %>
                <%= x.name %>: <%= x.typeRef ? `${x.typeRef.name}Contract` : x.type %>,
            <% }) %>
        }, returns?: any): Promise<FlowStateType & {returns: <%= operation.returnParameter.typeRef ? `${operation.returnParameter.typeRef.name}Contract` : operation.returnParameter.type %><%= operation.isReturnArray ? '[]' : '' %> | null}> {

        this.notifyComplete('<%= entity.name %>::<%= operation.name %>', state.start);
        return {...<FlowStateType>state, ...{returns: returns}};
    }
<% }) %>
