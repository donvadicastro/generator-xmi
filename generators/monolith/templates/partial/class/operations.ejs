<% (entity.operations || []).forEach(function (operation) { %>
    /**
    * <%= operation.name %> description.
    */
    <%= operation.name %>(state: FlowStateType & {
            <% operation.inputParameters.forEach(x => { %>
                <%= x.name %>: <%= x.typeRef ? `${x.typeRef.name}Contract` : x.type %>,
            <% }) %>
        }): Promise<FlowStateType & {returns: <%= operation.returnParameter.typeRef ? `${operation.returnParameter.typeRef.name}Contract` : operation.returnParameter.type %><%= operation.isReturnArray ? '[]' : '' %> | null}> {

        return new Promise((resolve, reject) => {
            this.notifyComplete('<%= entity.name %>::<%= operation.name %>', state.start);
            resolve({...<FlowStateType>state, ...{returns: null}});
        });
    }
<% }) %>
