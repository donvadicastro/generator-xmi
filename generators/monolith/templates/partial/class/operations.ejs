<% (entity.operations || []).forEach(function (operation) { %>
    /**
    * <%= operation.name %> description.
    */
    <%= operation.name %>(state: any): Promise<any> {
        return new Promise((resolve, reject) => {
            this.notifyComplete('<%= entity.name %>::<%= operation.name %>', state.start);
            resolve(state);
        });
    }
<% }) %>
