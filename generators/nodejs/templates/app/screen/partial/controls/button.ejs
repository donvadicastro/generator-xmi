<div class="form-group">
    <% if(entity.links.informationFLow.length && entity.links.informationFLow[0].end) {%>
        <% var elementRefPath = entity.links.informationFLow[0].end.elementRef.getPathFromRoot(utils.kebabCase) %>
        <button class="btn btn-primary" (click)="run('<%= elementRefPath %>')"><%= entity.properties.get('label') || entity.name %></button>
    <% } else { %>
        <button class="btn btn-primary" disabled>Run</button>
    <% } %>
</div>
