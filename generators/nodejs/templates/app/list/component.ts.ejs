import { Component } from '@angular/core';
import { HttpClient } from '@angular/common/http';
import {environment} from "../../../<%= entity.relativeRoot %>/environments/environment";

@Component({
  selector: 'app-<%= entity.name %>-list',
  templateUrl: './component.html',
  styleUrls: ['./component.sass']
})
export class <%= entity.namePascal %>ListComponent {
  data = [];

  constructor(private http: HttpClient) {}

  ngOnInit(): void {
    this.http.get(environment.api.url + '/<%= entity.getPathFromRoot(utils.kebabCase) %>/<%= utils.kebabCase(entity.name) %>').subscribe(res => this.data = <any>res);
  }
}
