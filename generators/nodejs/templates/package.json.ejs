{
    "name": "dist",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "author": "",
    "license": "ISC",
    "scripts": {
        "build": "tsc",
        "test": "jest",
        "api:start": "npm run api:compile && node dist/api/server/index.js",
        "api:start:prod": "NODE_ENV=production npm run api:compile && pm2 start dist/api/server/index.js",
        "api:start:forever": "npm run api:compile && forever start dist/api/server/index.js",
        "api:compile": "ts-node build.ts",
        "api:dev": "nodemon api/server/index.ts | pino -o 'short'",
        "api:dev:debug": "nodemon --exec 'node -r ts-node/register --inspect-brk' api/server/index.ts  | pino -o 'short'",
        "api:test": "mocha api/test/**/*.ts --exit",
        "api:test:debug": "mocha --inspect-brk api/test/**/*.ts --exit",
        "api:docker:build": "npm run api:compile && docker build -t generator-xmi-runner-nodejs/api -f api/Dockerfile .",
        "api:docker:run": "docker run -p 3000:3000 generator-xmi-runner-nodejs/api",
        "app:ng": "ng",
        "app:start": "ng serve --host 0.0.0.0",
        "app:start:forever": "forever start node_modules/@angular/cli/bin/ng serve --host 0.0.0.0",
        "app:build": "ng build --outputPath=dist/app_compiled",
        "app:build:prod": "ng build --outputPath=dist/app_compiled --prod",
        "app:test": "ng test",
        "app:docker:build": "npm run app:build && docker build -t generator-xmi-runner-nodejs/app -f app/Dockerfile .",
        "app:docker:run": "docker run -p 4200:80 generator-xmi-runner-nodejs/app",
        "all:docker:build": "tsc -v && ts-node -v && npm run api:docker:build && npm run app:docker:build",
        "all:docker:run": "npm run all:docker:build && docker-compose up",
        "lint": "ng lint",
        "e2e": "protractor e2e/protractor.conf.js",
        "jira:sync": "ts-node cmd/jira_sync.ts"
    },
    "jest": {
        "testEnvironment": "node",
        "verbose": false,
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    },
