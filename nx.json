{
  "targetDefaults": {
    "compile": {
      "inputs": [
        {
          "runtime": "node -v"
        }
      ],
      "outputs": [
        "{projectRoot}/lib"
      ]
    },
    "generate": {
      "inputs": [
        {
          "runtime": "node -v"
        }
      ],
      "outputs": [
        "{workspaceRoot}/output"
      ]
