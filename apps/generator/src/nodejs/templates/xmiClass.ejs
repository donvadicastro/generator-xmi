import {Entity, EntitySubscriberInterface, EventSubscriber} from "typeorm";
import {<%= entity.namePascal %>Base} from './generated/<%= entity.name %>.generated';

<%- include('partial/common/classDescription', {entity: entity, system: false}) -%>

@Entity({ name: '<%= entity.elementId %>' })
export class <%= entity.namePascal %> extends <%= entity.namePascal %>Base {
}

@EventSubscriber()
export class <%= entity.namePascal %>Subscriber implements EntitySubscriberInterface<<%= entity.namePascal %>> {
    listenTo() { return <%= entity.namePascal %>; }
}
